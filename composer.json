        {
            "name": "Evert Pot",
            "email": "me@evertpot.com",
            "homepage": "http://evertpot.com/",
            "role": "Developer"
        }
    ],
    "support": {
        "forum": "https://groups.google.com/group/sabredav-discuss",
        "source": "https://github.com/fruux/sabre-skel"
    },
    "autoload": {
        "psr-4" : {
            "Sabre\\Xml\\Atom\\" : "lib/"
